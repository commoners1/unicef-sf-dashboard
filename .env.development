# Development Environment Configuration
# This file is automatically loaded by Vite when running in development mode
VITE_ROUTER_BASENAME=/dashboard

# API Configuration
VITE_API_URL=http://localhost:3000
VITE_WS_URL=ws://localhost:3000

# Application Configuration
VITE_APP_TITLE=SF Middleware Dashboard
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=development

# API Timeout (in milliseconds)
VITE_API_TIMEOUT=10000

# Feature Flags
VITE_ENABLE_WEBSOCKETS=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_LOGS=true

# Authentication
VITE_AUTH_TOKEN_KEY=jwt_token
VITE_USER_PROFILE_KEY=user_profile

# Dashboard Configuration
VITE_DEFAULT_PAGE_SIZE=25
VITE_MAX_PAGE_SIZE=100
VITE_AUTO_REFRESH_INTERVAL=15000

# Logging Configuration
VITE_LOG_LEVEL=debug
VITE_MAX_LOG_ENTRIES=500

# Queue Configuration
VITE_QUEUE_REFRESH_INTERVAL=3000
VITE_MAX_QUEUE_JOBS_DISPLAY=50

# Export Configuration
VITE_MAX_EXPORT_ROWS=10000
VITE_EXPORT_FORMATS=json,csv,xlsx
