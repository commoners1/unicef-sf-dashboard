# Production Environment Configuration
# This file is automatically loaded by Vite when building for production

# API Configuration
VITE_API_URL=https://api.sf-middleware.com
VITE_WS_URL=wss://api.sf-middleware.com

# Application Configuration
VITE_APP_TITLE=SF Middleware Dashboard
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=production

# API Timeout (in milliseconds)
VITE_API_TIMEOUT=30000

# Feature Flags
VITE_ENABLE_WEBSOCKETS=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG_LOGS=false

# Authentication
VITE_AUTH_TOKEN_KEY=jwt_token
VITE_USER_PROFILE_KEY=user_profile

# Dashboard Configuration
VITE_DEFAULT_PAGE_SIZE=25
VITE_MAX_PAGE_SIZE=100
VITE_AUTO_REFRESH_INTERVAL=30000

# Logging Configuration
VITE_LOG_LEVEL=warn
VITE_MAX_LOG_ENTRIES=1000

# Queue Configuration
VITE_QUEUE_REFRESH_INTERVAL=5000
VITE_MAX_QUEUE_JOBS_DISPLAY=100

# Export Configuration
VITE_MAX_EXPORT_ROWS=100000
VITE_EXPORT_FORMATS=json,csv,xlsx
